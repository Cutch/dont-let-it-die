  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@prettier/plugin-php": "^0.19.1",
    "prettier": "^2.7.1",
    "sass": "^1.84.0",
    "typescript": "^5.7.3"
  },
  "prettier": {
    "braceStyle": "1tbs"
  }
}